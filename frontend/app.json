    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#059669"
      },
      "package": "com.hieudang0706.TinTro"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e93ac21f-03ab-409b-a0d1-06ccb114bb11"
      }
    }
  }
}
